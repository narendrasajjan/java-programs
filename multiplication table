import java.util.Scanner;
class Main {
    public static void main(String[] args) {
        int number;
        System.out.print("enter number : ");
        Scanner sc = new Scanner(System.in);
        number = sc.nextInt();
        for(int i = 1;i<10;i++)
        System.out.println(number + " * " + i + " = " + (number * i));
       
    }
}
